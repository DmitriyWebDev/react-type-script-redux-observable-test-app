    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^2.1.1",
    "jest-enzyme": "^7.1.2",
    "jest-resolve": "^24.0.0",
    "jest-serializer-html-string": "^1.0.1",
    "jest-styled-components": "^6.3.1",
