    "redux-mock-store": "^1.5.3",
    "sass-loader": "^6.0.6",
    "sinon": "^9.0.2",
    "source-list-map": "^2.0.0",
    "style-loader": "^0.18.2",
    "styled-components": "^4.1.1",
    "thread-loader": "^2.1.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typescript-plugin-css-modules": "^2.1.2",
    "uglify-js": "^3.0.28",
    "url-loader": "^0.5.9",
    "utility-types": "^3.10.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint-fix",
