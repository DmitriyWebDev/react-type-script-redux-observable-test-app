{
  "private": false,
  "name": "react-type-script-redux-observable-test-app",
  "version": "0.0.0",
  "scripts": {
    "start-mock-dev": "concurrently \"npm run api-express\" \"npm run dev-mock\"",
    "api-express": "nodemon server.js",
    "build": "webpack --config webpack.dev.js",
    "dev-mock": "cross-env HOST=http://localhost:3001 webpack-dev-server --open --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "test": "node scripts/test.js --watchAll",
    "test-ci": "node scripts/test.js --ci",
    "test-latest": "node scripts/test.js",
    "prettier": "prettier --write \"server_api/**/*.{js}\"",
    "prettier-ts": "prettier --parser typescript --write \"app/**/*.ts?(x)\"",
    "lint": "eslint server_api",
    "lint-fix": "eslint server_api --fix",
    "ts-lint": "tslint \"app/**/*.ts?(x)\"",
    "ts-lint-fix": "tslint --fix \"app/**/*.ts?(x)\"",
    "ui-kit-dev": "start-storybook",
    "ui-kit-build": "build-storybook -c .storybook -o ui-kit-build"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "jest-canvas-mock",
      "<rootDir>/scripts/jestSetupFiles/configureEnzyme"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/vendor",
      "/__snapshots"
    ],
    "globals": {
      "NODE_ENV": "dev",
      "state": {
        "global": {
          "user": ""
        }
      },
      "FRONTEND_APP_VERSION": ""
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "snapshotSerializers": [
      "jest-serializer-html-string"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "app/**/*.{ts,tsx}"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/app/**/?(*.)(spec|test).{js,jsx}",
      "<rootDir>/app/**/__tests__/**/*.{ts,tsx}",
      "<rootDir>/app/**/?(*.)(spec|test).{ts,tsx}"
    ],
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "ts",
      "tsx"
    ],
    "roots": [
      "<rootDir>/app"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
